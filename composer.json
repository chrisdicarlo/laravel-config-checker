{
    "name": "chrisdicarlo/laravel-config-checker",
    "description": "Package to check that configuration key references actually exist in your config files.",
    "keywords": [
        "Chris Di Carlo",
        "laravel",
        "laravel-config-checker"
    ],
    "homepage": "https://github.com/chrisdicarlo/laravel-config-checker",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Di Carlo",
            "email": "chris@dicarlosystems.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1||^8.2||^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "ChrisDiCarlo\\LaravelConfigChecker\\": "src/",
            "ChrisDiCarlo\\LaravelConfigChecker\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisDiCarlo\\LaravelConfigChecker\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-config-checker --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisDiCarlo\\LaravelConfigChecker\\LaravelConfigCheckerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
